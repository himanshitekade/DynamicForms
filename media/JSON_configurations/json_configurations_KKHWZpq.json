{
    "css":"base_configuration.css",
    "legend": "Json configurations",
    "HTML_fields":[
        {
            "tag":"input",
            "type":"file",
            "label":"Upload json:",
            "id":"file",
            "class": "form-control",
            "name":"file",
            "required":"true",
            "multiple":"false",
            "autofocus":"false",
            "accept":".json"
        },
        {
            "tag":"input",
            "type":"text",
            "label":"Form name:",
            "id":"form_name",
            "class":"form-control",
            "name":"form_name",
            "placeholder":"Enter name of the form",
            "required":"true"
        },
        {
            "tag":"input",
            "type":"text",
            "label":"Version:",
            "id":"version",
            "class":"form-control",
            "name":"version",
            "placeholder":"Enter version of the form",
            "required":"true"
        },
        {
            "tag":"button",
            "type":"submit",
            "class": "btn btn-success",
            "name": "button", 
            "value":"button",
            "disabled":"false",
            "label":"Submit"
        }
    ],
    "HTML_table":{
            "headers":["Name","Version"],
            "values":[
                {
                    "name":"form_name",
                    "type":"text"
                },
                {

                    "name":"version",
                    "type":"text"
                }
            ]
        }
}
